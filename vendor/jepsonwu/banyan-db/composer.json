{
  "name": "jepsonwu/banyan-db",
  "description": "banyanDB library",
  "keywords": [
    "banyan",
    "ssdb",
    "jepson"
  ],
  "homepage": "https://github.com/jepsonwu/banyan-db",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "jepsonwu",
      "email": "wjp13671142513@163.com"
    }
  ],
  "require": {
    "php": "^5.6 || ^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.5",
    "jakub-onderka/php-parallel-lint": "0.9"
  },
  "autoload": {
    "psr-4": {
      "Jepsonwu\\banyanDB\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jepsonwu\\banyanDB\\": "tests"
    }
  },
  "scripts": {
    "test": [
      "parallel-lint . --exclude vendor",
      "phpunit"
    ]
  }
}
