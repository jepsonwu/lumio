{
    "name": "jiuyan/common-component",
    "description": "通用模块",
    "authors": [
        {
            "name": "xinghuo",
            "email": "xinghuo@In66.com"
        }
    ],
    "require": {
        "jiuyan/tools": "dev-master",
        "jiuyan/common-cache": "dev-master",
        "cuckoo/thrift_client": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7",
        "squizlabs/php_codesniffer": "^3.0",
        "mockery/mockery": "^0.9.9"
    },
    "repositories": [
        {
            "packagist": false
        },
        {
            "type": "composer",
            "url": "http://packagist.in66.cc"
        },
        {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    ],
    "autoload":{
        "classmap": [
            "src/InFramework/Libraries/Jthrift"
        ],
        "psr-4": {
            "Jiuyan\\Common\\Component\\InFramework\\": "src/InFramework",
            "Jiuyan\\Common\\Component\\": "src/main"
        }
    },
    "config":{
        "secure-http":false
    },
    "prefer-stable": true
}
